{
  "name": "az-arch-gen",
  "version": "0.1.0",
  "type": "module",
  "description": "AI-powered Azure architecture diagram generator for Draw.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "az-arch-gen": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "tsx src/cli.ts",
    "test": "tsx src/test.ts",
    "server": "tsx src/server.ts",
    "web:install": "cd web && npm install",
    "web:dev": "cd web && npm run dev",
    "dev:full": "concurrently \"npm run server\" \"npm run web:dev\""
  },
  "keywords": [
    "azure",
    "architecture",
    "diagram",
    "drawio",
    "ai",
    "generator"
  ],
  "author": "Mika",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "express-rate-limit": "^8.2.1",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
